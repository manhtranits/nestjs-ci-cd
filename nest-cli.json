{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "./",
  "compilerOptions": {
    "assets": [
      {
        "outDir": "dist/config",
        "include": "config/**/*",
        "watchAssets": true
      },
      {
        "outDir": "dist/src",
        "include": "./src/database/seeders/data/**/*",
        "watchAssets": true
      }
    ]
  },
  "entryFile": "src/main"
}
